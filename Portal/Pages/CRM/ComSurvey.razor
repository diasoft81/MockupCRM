@using Portal.Model
<MudText Typo="Typo.h6">Plan for Survey</MudText>
<MudSelect T="string" Label="Ready for Survey" @bind-Value="ReadyForSurvey">
    <MudSelectItem Value=@("Yes")>Yes</MudSelectItem>
    <MudSelectItem Value=@("No")>No</MudSelectItem>
    <MudSelectItem Value=@("Unknown")>Unknown</MudSelectItem>
</MudSelect>

@code {
    [Parameter] public LeadItem Lead { get; set; }
    string ReadyForSurvey { get; set; }
}
